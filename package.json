{
  "name": "mullet-with-jam",
  "version": "0.0.1",
  "description": "Mullet Stack: Facebook in the front. Assisted by Azure Functions API",
  "watch": {
    "test": "{src,test}/*.js"
  },
  "scripts": {
    "lint": "eslint . --ext .js",
    "format": "prettier --single-quote --write *.js src/components/*.js test/*.js",
    "build": "webpack",
    "start": "node walmart.js"
  },
  "main": "walmart.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lynnaloo/mullet-with-jam.git"
  },
  "keywords": [
    "react",
    "Facebook",
    "mullet",
    "jamstack",
    "static"
  ],
  "author": "Linda Nichols",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lynnaloo/mullet-with-jam/issues"
  },
  "homepage": "http://www.mullet.run",
  "dependencies": {
    "@hapi/hapi": "^19.1.1",
    "@hapi/inert": "^6.0.1",
    "babel": "6.23.0",
    "babel-core": "6.4.0",
    "babel-jest": "21.0.0",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "body-parser": "1.19.0",
    "jest": "21.0.0",
    "npm-watch": "0.2.0",
    "prop-types": "15.6.2",
    "react": "16.10.1",
    "react-dom": "16.10.1",
    "react-scripts": "3.1.2",
    "webpack": "3.0.0"
  },
  "devDependencies": {
    "eslint": "6.3.0",
    "eslint-config-eta": "1.0.0",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-react": "7.16.0",
    "jest-cli": "21.0.0",
    "prettier": "2.0.4",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.5.2",
    "nodemon": "*"
  }
}
